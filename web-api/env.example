# SESAME Device Configuration
# Get these values from your SESAME device QR code using: https://sesame-qr-reader.vercel.app/
SESAME_BLE_UUID=your-device-ble-uuid-here
SESAME_SECRET_KEY=your-secret-key-here
SESAME_PUBLIC_KEY=your-public-key-here

# Optional: Scan duration in seconds (default: 15)
SESAME_SCAN_DURATION=15

# Server Configuration
HOST=0.0.0.0
PORT=8000
DEBUG=false

# Logging
LOG_LEVEL=INFO
